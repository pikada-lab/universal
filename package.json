{
  "name": "aptechki.ru",
  "version": "1.0.0",
  "description": "Aptechki for your business",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular.io-example:serve-ssr --port=4201",
    "serve:ssr": "node dist/server/main.js",
    "copy:imgbuilder": "cp -r ./node_modules/webp-converter/bin ./dist",
    "build:ssr": "ng build && ng run angular.io-example:server && npm run copy:imgbuilder",
    "prerender": "ng run angular.io-example:prerender",
    "dockerize": "docker build . -t aptechki.ru",
    "deploy": "npm run build:ssr && rm -r ./deploy && cp -r ./dist ./deploy && pm2 restart 0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/platform-server": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@nguniversal/express-engine": "~13.0.0",
    "angular-in-memory-web-api": "~0.11.0",
    "angular-markdown-directive": "^0.3.1",
    "cardinal": "^2.1.1",
    "express": "^4.15.2",
    "express-fileupload": "^1.3.1",
    "icq-bot": "^2.1.45",
    "markdown-to-html": "^0.0.13",
    "marked-id-no-overlap": "^1.0.3",
    "mysql2": "^2.3.3",
    "ngx-markdown": "^13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "webp-converter": "^2.3.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.1",
    "@angular/cli": "~13.2.1",
    "@angular/compiler-cli": "~13.2.0",
    "@nguniversal/builders": "~13.0.0",
    "@types/express": "^4.17.8",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "jasmine-marbles": "~0.8.3",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.1.0",
    "typescript": "~4.5.2"
  }
}
